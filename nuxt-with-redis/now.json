{
  "version": 2,
  "cleanUrls": true,
  "builds": [
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/vercel-builder"
    },
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/count",
      "destination": "/api/count.js"
    }
  ]
}
